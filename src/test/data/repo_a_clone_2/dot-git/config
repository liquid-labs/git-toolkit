	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
	ignorecase = true
	precomposeunicode = true
[remote "upstream"]
	url = ../repo_a
	fetch = +refs/heads/*:refs/remotes/upstream/*
[branch "main"]
	remote = upstream
	merge = refs/heads/main
[remote "coworker"]
	url = file://../repo_a_clone
	fetch = +refs/heads/*:refs/remotes/coworker/*
